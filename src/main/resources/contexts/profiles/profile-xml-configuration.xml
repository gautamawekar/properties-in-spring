<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-4.0.xsd">
    
    <beans profile="test1">
        <util:map id="fullName">
            <entry key="name" value="gautam from profile test1"/>
            <entry key="surname" value="Awekar from profile test1"/>
        </util:map>
    </beans>
    
    <beans profile="test2">
        <util:map id="fullName-test2">
            <entry key="name" value="gautam from profile test2"/>
            <entry key="surname" value="Awekar from profile test2"/>
        </util:map>
    </beans>
    
    <beans profile="test3">
        <util:map id="fullName">
            <entry key="name" value="gautam from profile test3"/>
            <entry key="surname" value="Awekar from profile test3"/>
        </util:map>
    </beans>

</beans>
